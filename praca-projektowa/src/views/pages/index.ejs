<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Users</title>
  </head>
  <body>
    <h1>Users</h1>

    <a href="/add">Add user</a>

    <ul>
      <% if (Array.isArray(notes) && notes.length) { %>
        <% notes.forEach(function (u) { %>
          <li>
            <a href="/users/<%= encodeURIComponent(u.username) %>">
              <%= u.username %>
            </a>
          </li>
        <% }) %>
      <% } else { %>
        <li>No users yet.</li>
      <% } %>
    </ul>
  </body>
</html>